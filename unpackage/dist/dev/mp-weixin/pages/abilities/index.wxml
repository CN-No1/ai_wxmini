<view class="abilities"><image class="banner" src="../../static/banner/banner2.png"></image><view class="abilities-list"><u-sticky vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><view class="sticky"><xfl-select vue-id="{{('2')+','+('1')}}" list="{{list}}" clearable="{{false}}" showItemNum="{{5}}" listShow="{{false}}" isCanInput="{{false}}" style_Container="height: 50px; font-size: 16px;" placeholder="能力分类" selectHideType="hideAll" data-event-opts="{{[['^change',[['check']]]]}}" bind:change="__e" bind:__l="__l"></xfl-select></view></u-sticky><block wx:for="{{abilitiesList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-ref="{{item.name}}" class="abilities-wrapper vue-ref-in-for"><view class="title">{{''+item.name+''}}</view><view class="abilities-wrapper-inner"><block wx:for="{{item.abs}}" wx:for-item="i" wx:for-index="index" wx:key="index"><view class="abilities-item"><image class="abilities-icon" src="{{i.img}}"></image><view class="abilities-name">{{''+i.name+''}}</view></view></block></view></view></block></view></view>